COUNTRIES = "China" "India"
rule male_population_plot:
  input:
    a = "../data/Agrofood_co2_emission.csv",
    b = "0",
    c1 = "1",
    c2 = "29",
    d = "Year" "Number of People",
    e = "Total Population Trend"
  output:
    COUNTRIES + '_{input.e}.png'
  shell:
    "python data_analysis.py --file_name {input.a} --country_list " + COUNTRIES + " --country_column int({input.b}) --result_column_list int({input.c1}) int({input.c2}) --x_y_labels {input.d} --plot_name {input.e}"